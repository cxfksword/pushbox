<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="20dp"/>
            <gradient android:startColor="#26c6da" android:endColor="#2196f3" android:angle="180" android:type="linear"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="20dp"/>
            <gradient android:startColor="#6d82f3" android:endColor="#1592f6" android:angle="180" android:type="linear"/>
        </shape>
    </item>
</selector>
